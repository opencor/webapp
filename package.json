{
  "author": {
    "name": "Alan Garny",
    "email": "a.garny@auckland.ac.nz"
  },
  "binary": {
    "napi_versions": [
      9
    ]
  },
  "bugs": {
    "url": "https://github.com/opencor/webapp/issues"
  },
  "description": "A Web-based modelling environment, which can be used to organise, edit, simulate and analyse CellML files.",
  "homepage": "https://opencor.ws/",
  "license": "Apache-2.0",
  "main": "./out/main/index.cjs",
  "name": "opencor",
  "private": true,
  "productName": "OpenCOR",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agarny/webapp.git"
  },
  "type": "module",
  "version": "0.20250626.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "electron",
      "electron-winstaller",
      "esbuild"
    ]
  },
  "scripts": {
    "build": "electron-vite build",
    "build:web": "cd ./src/renderer && vite build",
    "clean": "node scripts/clean.js",
    "dev": "electron-vite dev --watch",
    "dev:web": "cd ./src/renderer && vite dev",
    "format": "prettier --log-level silent --write . && clang-format -i src/libopencor/src/*",
    "format:check": "prettier --check . && clang-format --dry-run -Werror src/libopencor/src/*",
    "install": "node scripts/version.js",
    "libopencor": "cmake-js build -B Release -O dist/libOpenCOR",
    "lint": "eslint --fix .",
    "package": "pnpm build && electron-builder",
    "publish:web": "pnpm publish ./src/renderer",
    "start": "electron-vite preview",
    "start:web": "pnpm build:web && cd ./src/renderer && vite preview"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@primeuix/themes": "^1.1.2",
    "@primevue/auto-import-resolver": "^4.3.5",
    "@tailwindcss/postcss": "^4.1.10",
    "@tailwindcss/vite": "^4.1.10",
    "@types/node": "^24.0.4",
    "@types/plotly.js": "^3.0.2",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.1",
    "@vue/tsconfig": "^0.7.0",
    "@vueuse/core": "^13.4.0",
    "@wasm-fmt/clang-format": "^20.1.7",
    "autoprefixer": "^10.4.21",
    "cmake-js": "^7.3.1",
    "electron": "^37.0.0",
    "electron-builder": "^26.0.12",
    "electron-settings": "^4.0.4",
    "electron-updater": "^6.6.2",
    "electron-vite": "^3.1.0",
    "eslint": "^9.29.0",
    "jsonschema": "^1.5.0",
    "libopencor": "https://opencor.ws/libopencor/downloads/libopencor-0.20250613.0.tgz",
    "mathjs": "^14.5.2",
    "node-addon-api": "^8.4.0",
    "plotly.js-gl2d-dist-min": "^3.0.1",
    "prettier": "^3.6.1",
    "primeicons": "^7.0.0",
    "primevue": "^4.3.5",
    "quill": "^2.0.3",
    "systeminformation": "^5.27.6",
    "tailwindcss": "^4.1.10",
    "ua-parser-js": "^2.0.4",
    "unplugin-vue-components": "^28.7.0",
    "vite": "^6.3.5",
    "vue": "^3.5.17"
  }
}
